{
	"name": "vscode-vuex-peek",
	"displayName": "vuex peek",
	"description": "",
	"version": "0.6.0",
	"publisher": "Mcbai",
  "repository": {
    "type": "git",
    "url": "https://github.com/Mcbai/vscode-vuex-peek"
  },
	"engines": {
		"vscode": "^1.33.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:vue"
	],
	"main": "./extension.js",
	"contributes": {
    "configuration": {
      "title": "store path",
      "properties": {
        "vuex_peek.storePath": {
          "type": "array",
          "default": ["src/"],
					"items": {
							"type": "string"
					},
          "description": "A list for the path of store folder."
        }
			}
		}
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"eslint": "^5.13.0",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	}
}
